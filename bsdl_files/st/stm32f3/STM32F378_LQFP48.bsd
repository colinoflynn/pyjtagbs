	  PB6           : inout bit;
	  PB7           : inout bit;
	  PB8           : inout bit;
	  PB9           : inout bit;
	  PB14          : inout bit;
	  PB15          : inout bit;
	  PC13          : inout bit;
	  OSCIN         : inout bit;
	  OSCOUT        : inout bit;
	  PD8           : inout bit;
	  PE8           : inout bit;
	  PE9           : inout bit;
	  PC14_OSC32IN  : inout bit;
	  PC15_OSC32OUT : inout bit;
	  PF6           : inout bit;
	  PF7           : inout bit;
             VBAT          : linkage bit;
	  VDD           : linkage bit_vector(0 to 1);
             VDDA          : linkage bit;
             VREFPLUS      : linkage bit;
             VREFSDP       : linkage bit;
             VSSSD         : linkage bit;
             VDDSD         : linkage bit;
             VSS           : linkage bit;
             VSSA          : linkage bit
