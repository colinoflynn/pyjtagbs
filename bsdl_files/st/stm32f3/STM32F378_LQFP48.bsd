 entity STM32F378_LQFP48 is
 -- This section identifies the default device package selected.
 generic (PHYSICAL_PIN_MAP: string:= "LQFP48_PACKAGE");
 -- This section declares all the ports in the design.
   port ( 
	  BOOT          : in bit;
	  JTDI          : in bit;
	  JTMS          : in bit;
	  JTCK          : in bit;
	  JTRST         : in bit;
	  JTDO          : out bit;
	  NRST          : in bit;	-- modification to add COMPLIANCE_PATTERNS 
	  PA0           : inout bit;
	  PA1           : inout bit;
	  PA2           : inout bit;
	  PA3           : inout bit;
	  PA4           : inout bit;
	  PA5           : inout bit;
	  PA6           : inout bit;
	  PA8           : inout bit;
	  PA9           : inout bit;
